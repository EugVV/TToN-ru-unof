    <Entry>
      <ID>7</ID>
      <DefaultText>"Прощай".</DefaultText>
      <FemaleText>"Прощай".</FemaleText>
    </Entry>
    <Entry>
      <ID>8</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>9</ID>
      <DefaultText>"Расскажи мне, как ты сюда попал?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>10</ID>
      <DefaultText>"Как ты сюда попал, говоришь?"</DefaultText>
      <FemaleText>"Как ты сюда попал, говоришь?"</FemaleText>
