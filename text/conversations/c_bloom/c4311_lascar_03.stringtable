      <ID>18</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>19</ID>
      <DefaultText>"Что ты делаешь?"</DefaultText>
      <FemaleText>"Ты что делаешь?</FemaleText>
    </Entry>
