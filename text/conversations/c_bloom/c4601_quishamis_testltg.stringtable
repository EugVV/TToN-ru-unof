    <Entry>
      <ID>28</ID>
      <DefaultText>Уйти.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>29</ID>
      <DefaultText>Атаковать.</DefaultText>
      <FemaleText />
