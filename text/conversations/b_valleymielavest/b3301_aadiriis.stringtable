      <ID>17</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>18</ID>
      <DefaultText>"Расскажи о себе".</DefaultText>
      <FemaleText>"Расскажи о себе".</FemaleText>
    </Entry>
    <Entry>
      <ID>19</ID>
      <DefaultText>"Поговорим о тебе еще".</DefaultText>
