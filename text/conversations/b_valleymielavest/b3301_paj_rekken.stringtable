    <Entry>
      <ID>30</ID>
      <DefaultText>"Я тебя знаю. Я знаю, что ты сделала в деревушке Кольн".</DefaultText>
      <FemaleText>"Я тебя знаю. Я знаю, что ты сделала в деревушке Кольн".</FemaleText>
    </Entry>
    <Entry>
      <ID>31</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>32</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
