      <ID>406</ID>
      <DefaultText />
      <FemaleText />
    </Entry>
    <Entry>
      <ID>407</ID>
      <DefaultText>«Что теперь? Что тебе ещё от меня нужно?»</DefaultText>
      <FemaleText />
    </Entry>
