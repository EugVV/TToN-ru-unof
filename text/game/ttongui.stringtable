      <DefaultText>Призывает потусторонние силы</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>340</ID>
      <DefaultText>Много понимает</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
