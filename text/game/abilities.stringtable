      <DefaultText>Бой мага</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>1087</ID>
      <DefaultText>Приобретите [StatModifiers] и [SimpleTaskModifiers].</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
