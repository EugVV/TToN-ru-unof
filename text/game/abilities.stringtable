    <Entry>
      <ID>858</ID>
      <DefaultText>+25% к вероятности нанесения критического удара.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>859</ID>
      <DefaultText>Внезапная атака</DefaultText>
      <FemaleText />
